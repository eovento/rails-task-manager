<h1>All tasks:</h1>

<% @tasks.each do |task| %>
  <h2><%= task.id %>- <%= task.name %></h2>
  <%= link_to "see details", task_path(task) %>
  <%= link_to "edit", "tasks/:id/edit" %>
  <%= link_to "delete", ":id/" %>
  <br>
<% end %>

<br>

<div id="create">
<a href="new">Create a new Task</a>
</div>